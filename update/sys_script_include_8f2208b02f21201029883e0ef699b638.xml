        <api_name>x_58872_needit.lookupEmailAddress</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Looks up the email address for  the requested_for_email field. Non Ajax, not client callerble</description>
        <name>lookupEmailAddress</name>
        <script><![CDATA[var lookupEmailAddress = Class.create();
lookupEmailAddress.prototype = {
    initialize: function() {},
	
	getEmail: function(userID) {
		var userRecord = new GlideRecord("sys_user");
		userRecord.get(userID);	       	
		
		
		return userRecord.email + '';
    },
			
    type: 'lookupEmailAddress'
};]]></script>

    <sys_script_client action="DELETE">
        <active>false</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Only display the matching options for the request type.</description>
        <field>u_request_type</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>RequireIT Request Type Options</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (newValue == '') {
        return;
    }

    var optDefault = g_form.getValue('u_what_needed');
    g_form.clearOptions('u_what_needed');

    if (newValue == 'hw') {
        //Add Hardware Items
        g_form.addOption('u_what_needed', 'desktop', 'Desktop');
        g_form.addOption('u_what_needed', 'laptop', 'Laptop');
        g_form.addOption('u_what_needed', 'pprinter', 'Personal Printer');
    }

    if (newValue == 'sw') {
        //Add Hardware Items
        g_form.addOption('u_what_needed', 'office', 'MS Office');
        g_form.addOption('u_what_needed', 'antivirus', 'McAfee Antivirus');
        g_form.addOption('u_what_needed', 'studio', 'Visual Studio');
    }

    if (newValue == 'rm') {
        g_form.addOption('u_what_needed', 'vpn', 'VPN Setup');
        g_form.addOption('u_what_needed', 'internet', 'Internet Setup');
    }

    if (newValue == 'security') {
        //Add Hardware Items
        g_form.addOption('u_what_needed', 'useracct', 'User Account');
        g_form.addOption('u_what_needed', 'pwdreset', 'Password Reset');
    }

    if (newValue == 'emp') {
        //Add Hardware Items
        g_form.addOption('u_what_needed', 'newemp', 'Employee Access');
        g_form.addOption('u_what_needed', 'bgcheck', 'Background Check');
        g_form.addOption('u_what_needed', 'cjis', 'CJIS');
    }

	if (newValue == 'security') {
		g_form.addOption('u_what_needed', 'useracct', 'User Account');
        g_form.addOption('u_what_needed', 'pwdreset', 'Password Reset');
	}
    // value from the record before it was loaded
    if (isLoading && !g_form.isNewRecord()) {
        g_form.setValue('u_what_needed', optDefault);
    }




}]]></script>
